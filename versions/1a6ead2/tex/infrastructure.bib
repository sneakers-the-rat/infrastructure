@article{simaEnablingSemanticQueries2019,
  title = {Enabling Semantic Queries across Federated Bioinformatics Databases},
  author = {Sima, Ana Claudia and {Mendes de Farias}, Tarcisio and Zbinden, Erich and Anisimova, Maria and Gil, Manuel and Stockinger, Heinz and Stockinger, Kurt and {Robinson-Rechavi}, Marc and Dessimoz, Christophe},
  year = {2019},
