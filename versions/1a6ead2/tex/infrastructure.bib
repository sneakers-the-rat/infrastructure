@article{randallDistributedOntologyBuilding2011,
  title = {Distributed Ontology Building as Practical Work},
  author = {Randall, Dave and Procter, Rob and Lin, Yuwei and Poschen, Meik and Sharrock, Wes and Stevens, Robert},
