  title = {{{IPLS}} : {{A Framework}} for {{Decentralized Federated Learning}}},
  shorttitle = {{{IPLS}}},
  author = {Pappas, Christodoulos and Chatzopoulos, Dimitris and Lalis, Spyros and Vavalis, Manolis},
