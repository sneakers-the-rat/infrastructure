  title = {{{DeepSqueak}}: A Deep Learning-Based System for Detection and Analysis of Ultrasonic Vocalizations},
  shorttitle = {{{DeepSqueak}}},
  author = {Coffey, Kevin R. and Marx, Russell G. and Neumaier, John F.},
  year = {2019},
