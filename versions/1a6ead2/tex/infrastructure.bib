@article{ribesOrganizingOntologicalChange2015,
  title = {Organizing for Ontological Change: {{The}} Kernel of an {{AIDS}} Research Infrastructure},
  shorttitle = {Organizing for Ontological Change},
  author = {Ribes, David and Polk, Jessica Beth},
  year = {2015},
  month = apr,
  journal = {Social Studies of Science},
  volume = {45},
